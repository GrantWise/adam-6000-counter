{
  "id": "adam-4571-basic",
  "name": "ADAM-4571 Basic Weight",
  "manufacturer": "Advantech",
  "model": "ADAM-4571",
  "description": "Basic weight reading protocol for ADAM-4571 scale interface",
  "commands": [
    "W",
    "#01"
  ],
  "expectedResponses": [
    "[\\+\\-]?\\d+\\.?\\d*",
    ">\\+?\\d+\\.?\\d*"
  ],
  "weightPattern": "([\\+\\-]?\\d+\\.?\\d*)",
  "unit": "kg",
  "settings": {
    "baudRate": 9600,
    "dataBits": 8,
    "stopBits": 1,
    "parity": "None",
    "readTimeout": 5000,
    "writeTimeout": 5000
  }
}