{
  "id": "generic-scale",
  "name": "Generic Scale Protocol",
  "manufacturer": "Generic",
  "description": "Generic protocol for common scale devices",
  "commands": [
    "W\r\n",
    "P\r\n",
    "?\r\n",
    "PRINT\r\n"
  ],
  "expectedResponses": [
    "\\d+\\.?\\d*",
    "[\\+\\-]?\\d+\\.?\\d*\\s*\\w*",
    "ST,\\w*,[\\+\\-]?\\d+\\.?\\d*"
  ],
  "weightPattern": "([\\+\\-]?\\d+\\.?\\d*)",
  "unit": "kg",
  "settings": {
    "baudRate": 9600,
    "dataBits": 8,
    "stopBits": 1,
    "parity": "None",
    "readTimeout": 3000,
    "writeTimeout": 3000
  }
}